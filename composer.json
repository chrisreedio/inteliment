{
    "name": "chrisreedio/inteliment",
    "description": "Filament Integration for OpenAI's Assistants",
    "keywords": [
        "chrisreedio",
        "laravel",
        "inteliment"
    ],
    "homepage": "https://github.com/chrisreedio/inteliment",
    "support": {
        "issues": "https://github.com/chrisreedio/inteliment/issues",
        "source": "https://github.com/chrisreedio/inteliment"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Chris Reed",
            "email": "chris@reedtech.us",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/filament": "^3.0",
        "illuminate/contracts": "^10.0",
        "openai-php/laravel": "v0.8.0-beta.1",
        "spatie/laravel-data": "^3.9",
        "spatie/laravel-package-tools": "^1.15.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.9",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "ChrisReedIO\\Inteliment\\": "src/",
            "ChrisReedIO\\Inteliment\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ChrisReedIO\\Inteliment\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true,
            "php-http/discovery": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ChrisReedIO\\Inteliment\\IntelimentServiceProvider"
            ],
            "aliases": {
                "Inteliment": "ChrisReedIO\\Inteliment\\Facades\\Inteliment"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
